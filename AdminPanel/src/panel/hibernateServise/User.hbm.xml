<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="panel.hibernateServise.User" table="route">
        <id column="id" name="id" type="integer">
            <generator class="increment"/>
        </id>
        <property column="from" name="from" type="string"/>
        <property column="where" name="where" type="string"/>
        <property column="when" name="when" type="string"/>
        <property column="mail" name="mail" type="string"/>
        <property column="vkId" name="vkId" type="string"/>
        <property column="comment" name="comment" type="string"/>
    </class>
</hibernate-mapping>